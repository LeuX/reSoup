{
  "manifest_version": 2,
  "name": "reSoup",
  "description": "This extension allows you to pause soup while real life needs your attention.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.soup.io/*"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.min.js",
        "reSoup.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
